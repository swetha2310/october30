pipeline{
    agent any
    environment {
        PATH = "$PATH:/opt/maven/apache-maven-3.9.0/bin"
    }
    stages{
       stage('Clone Code'){
            steps{
                git branch: 'master', url: 'https://github.com/swetha2310/october30.git'
            }
       }
       stage('Jira'){
            steps{
                jiraComment body: 'Testing this pipeline', issueKey: 'PA-1'
            }
       }
    }
}


